<h1>Sessions#signup</h1>

<%= form_for @photon, url: sessions_create_photon do |f| %>

<p>
  <%= f.label :photonname %>
  <%= f.text_field :photonname %>
</p>
<p>
  <%= f.label :password %>
  <%= f.text_field :password %>
</p>
<p>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>
</p>
<p>
  <%= f.submit %>
</p>

<% end %>
