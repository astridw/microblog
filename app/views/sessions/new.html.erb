<h1>Sessions#new</h1>

<%= form_tag sign_in_path do %>

  <p>
    <%= label_tag :photonname %>
    <%= text_field_tag :photonname %>
  </p>

  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>

  <p>
    <%= submit_tag "Sign In" %> or <%= link_to "Sign Up", signup_path %>
  </p>

<% end %>
